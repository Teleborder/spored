<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>sh.spore.spored</string>
  <key>EnvironmentVariables</key>
  <dict>
         <key>DEBUG</key>
         <string>spored</string>
         <key>PATH</key>
         <string>{{{NODE_BIN}}}:$PATH</string>
  </dict>
  <key>ProgramArguments</key>
  <array>
         <string>{{{NPM_BIN}}}/spored</string>
  </array>
  <key>StandardOutPath</key>
  <string>{{{SPORED_HOME}}}/log.log</string>
  <key>StandardErrorPath</key>
  <string>{{{SPORED_HOME}}}/error.log</string>
  <key>KeepAlive</key>
  <true/>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
